{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "standard"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "quotes":["error","single", {"allowTemplateLiterals":true}],
    "semi":["error","never"],
    "space-before-function-paren":["error","always"]
  }
}
